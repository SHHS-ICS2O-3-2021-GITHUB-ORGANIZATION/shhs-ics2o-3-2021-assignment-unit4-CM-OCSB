num2 = randint(0,100)
answer = (num1 + num2)
print ("what is " + str(num1))
print ("plus " + str(num2)) 
num3 = int(input())
print (answer)
if num3 ==(num1 + num2):
  print("you are correct!")
else:
  print("YOU ARE INCORRECT! TRY AGAIN!")
